<form string="Stock Out" col="6">
    <label name="reference"/>
    <field name="reference"/>
    <newline/>
    <label name="description"/>
    <field name="description" colspan="3"/>
    <notebook colspan="6">
        <page string="Stock" id="stock">
            <label name="stock_date"/>
            <field name="stock_date"/>
            <field name="lines" colspan="4"
                view_ids="nodux_stock_one.stock_out_line_view_tree,nodux_stock_one.stock_out_line_view_form"/>
            <group col="2" colspan="2" id="states">
                <label name="state"/>
                <field name="state"/>
            </group>
            <group col="2" colspan="2" id="amount_buttons">
                <group col="6" colspan="2" id="buttons">
                    <button name="done" string="Done"
                        icon="tryton-go-next"/>
                </group>
            </group>
        </page>
        <page string="Other Info" id="info">
            <label name="company"/>
            <field name="company"/>
        </page>
    </notebook>
</form>
